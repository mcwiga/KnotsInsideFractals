@article{https://doi.org/10.1002/anie.201702531,
	author = {Fielden, Stephen D. P. and Leigh, David A. and Woltering, Steffen L.},
	title = {Molecular Knots},
	journal = {Angewandte Chemie International Edition},
	volume = {56},
	number = {37},
	pages = {11166-11194},
	keywords = {interlocked molecules, molecular knots, supramolecular chemistry, template synthesis, topology},
	doi = {https://doi.org/10.1002/anie.201702531},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/anie.201702531},
	eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/anie.201702531},
	abstract = {Abstract The first synthetic molecular trefoil knot was prepared in the late 1980s. However, it is only in the last few years that more complex small-molecule knot topologies have been realized through chemical synthesis. The steric restrictions imposed on molecular strands by knotting can impart significant physical and chemical properties, including chirality, strong and selective ion binding, and catalytic activity. As the number and complexity of accessible molecular knot topologies increases, it will become increasingly useful for chemists to adopt the knot terminology employed by other disciplines. Here we give an overview of synthetic strategies towards molecular knots and outline the principles of knot, braid, and tangle theory appropriate to chemistry and molecular structure.},
	year = {2017}
} 
